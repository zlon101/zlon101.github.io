    "gatsby-remark-images": "6.10.2",
    "gatsby-source-filesystem": "2.2.2",
    "gatsby-transformer-remark": "2.7.1",
    "less": "4.1.1",
    "react": "16.12.0",
    "react-dom": "16.12.0"
